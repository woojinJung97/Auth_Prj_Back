<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kr.travel.springbootprj.home.mapper.AuthManagementMapper">
    <select id="authList" parameterType="com.kr.travel.springbootprj.home.dvo.AuthManagementDvo" resultType="com.kr.travel.springbootprj.home.dvo.AuthManagementDvo">
        SELECT *
          FROM roles a
         INNER JOIN users u
           ON a.id = u.role_id
    </select>
</mapper>
